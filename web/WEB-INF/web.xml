<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>insertUserServlet</servlet-name>
        <servlet-class>pck.users.insertUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getUsersServlet</servlet-name>
        <servlet-class>pck.users.getUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Users</servlet-name>
        <servlet-class>pck.users.Users</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>invertarioCar</servlet-name>
        <servlet-class>pck.users.invertarioCar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loadCar</servlet-name>
        <servlet-class>pck.users.loadCar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>product</servlet-name>
        <servlet-class>pck.users.product</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InventCarros</servlet-name>
        <servlet-class>pck.users.Carros</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getVehiculos</servlet-name>
        <servlet-class>pck.users.getVehiculos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertVehiculo</servlet-name>
        <servlet-class>pck.users.insertVehiculo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insertUserServlet</servlet-name>
        <url-pattern>/insertUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getUsersServlet</servlet-name>
        <url-pattern>/getUsersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Users</servlet-name>
        <url-pattern>/Users</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>invertarioCar</servlet-name>
        <url-pattern>/invertarioCar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadCar</servlet-name>
        <url-pattern>/loadCar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>product</servlet-name>
        <url-pattern>/product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InventCarros</servlet-name>
        <url-pattern>/InventCarros</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getVehiculos</servlet-name>
        <url-pattern>/getVehiculos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertVehiculo</servlet-name>
        <url-pattern>/insertVehiculo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
